<section id="Listing_View" >
<% unless @test.nil? %>
<h1>Search Results</h1>
<hr id="Listing_View_Bar"/>

<table>
  <tr>
    <th>Address</th>
    <th>City</th>
    <th>State</th>
    <th>Zip</th>
    <th>Region</th>
    <th>Bedrooms</th>
    <th>Bathrooms</th>
    <th>Rent</th>
    <th>Squarefeet</th>
    <th>Parking</th>
    <th>Smoking</th>
    <th>Pets</th>
    <th>Heat</th>
    <th>Electric</th>
    <th>Gas</th>
    <th>Garbage Collection</th>
    <th>Furnished</th>
    <th>Laundry</th>
    <th>Flags</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @lists.each do |list| %>
  <tr>
    <td><%= list.Address%></td>
    <td><%= list.City %></td>
    <td><%= list.State %></td>
    <td><%= list.Zip %></td>
    <td><%= list.Region %></td>
    <td><%= list.Bedrooms %></td>
    <td><%= list.Bathrooms %></td>
    <td><%= list.Rent %></td>
    <td><%= list.SquareFeet %></td>
    <td><%= list.Parking %></td>
    <td><%= list.Smoking %></td>
    <td><%= list.Pets %></td>
    <td><%= list.Heat %></td>
    <td><%= list.Electric %></td>
    <td><%= list.Gas %></td>
    <td><%= list.GarbageCollection %></td>
    <td><%= list.Furnished %></td>
    <td><%= list.Laundry %></td>
    <td><%= list.Flags %></td>
    <td><%= link_to 'Show', list %></td>
    <td><%= link_to 'Edit', edit_list_path(list) %></td>
    <td><%= link_to 'Destroy', list, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
